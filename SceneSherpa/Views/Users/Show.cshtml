@model SceneSherpa.Models.User
@{
    string currentUsernameId = ViewData["CurrentUserIdUsername"] as string;
    List<string> userObj = new List<string>();

    userObj.AddRange(currentUsernameId.Split());
}
<h1>Name: @Model.Name</h1>
<h1>Age: @Model.Age</h1>
<h1>Username: @Model.Username</h1>
<h1>Email: @Model.Email</h1>

<form action="/Users/@Model.Id/Delete" method="post">
    <button type="submit">DELETE @Model.Username</button>
</form>
<form action="/Users/@Model.Id/Edit">
    <button type="submit">EDIT @Model.Username</button>
</form>


<hr />
<h3>Library</h3>
<br />
<br />
<h2>Currently Watching:</h2>
@foreach(var Media in Model.CurrentWatch)
{
    <p>@Media.Type: @Media.Title <form><button type="submit">DELETE @Media.Title</button></form></p>
}
<br />
<h2>Watched:</h2>
@foreach (var Media in Model.AllWatched)
{
    <p>@Media.Type: @Media.Title <form><button type="submit">DELETE @Media.Title</button></form></p>
}
<br />
<h2>Want to watch:</h2>
@foreach (var Media in Model.ToWatch)
{
    <p>@Media.Type: @Media.Title <form><button type="submit">DELETE @Media.Title</button></form></p>
}